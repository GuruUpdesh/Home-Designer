{
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*).css",
        "dest": "/$1.css"
      },
      {
        "src": "/(.*).(jpg|png)",
        "dest": "/$1.$2"
      },
      {
        "src": "/(.*).json",
        "dest": "/$1.json"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  